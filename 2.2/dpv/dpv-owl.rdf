  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Likelihood">
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <skos:definition xml:lang="en">The likelihood or probability or chance of something taking place or occurring</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
