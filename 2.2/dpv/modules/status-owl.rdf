    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">State of being conditionally approved through the audit</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-29</dct:created>
    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AuditStatus"/>
  </rdf:Description>
