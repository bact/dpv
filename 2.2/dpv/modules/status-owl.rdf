  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ReuseCompatibility">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Status"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <dct:contributor rdf:nodeID="person-arthit-suriyawongkul"/>
    <skos:definition xml:lang="en">Concept indicating whether the specified context is compatible with another earlier context</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-06-19</dct:created>
    <skos:scopeNote xml:lang="en">Compatibility refers to whether two contexts, such as dpv:Process instances, are compatible with each other based on a criteria which is not defined by the concept. This criteria can be based on requirements from law, such as GDPR's purpose compatibility or can be based on privacy norms such as contextual integrity. To express such interpretations, further concepts should be expanded from this base concept</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">ReuseCompatibility</skos:prefLabel>
  </rdf:Description>
