  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RightNotice">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-06-16</dct:created>
