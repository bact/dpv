  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InverseJurisdiction">
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Jurisdiction"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Inverse Jurisdictions are helpful when asserting and checking rules that involve a concept representing jurisdictions outside of a given jurisdiction. An inverse jurisdiction of an inverse jurisdiction should give back the same original jurisdiction but this is undefined and strongly dissuaded as it could lead to recursive definitions in some use-cases</skos:scopeNote>
    <skos:definition xml:lang="en">An inverse jurisdiction for a specific jurisdiction is the set of all other jurisdictions that are not part of the specific jurisdiction</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-05-04</dct:created>
    <skos:prefLabel xml:lang="en">Inverse Jurisdiction</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Jurisdiction"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HybridPublicPrivateSpace">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Location"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-arthit-suriyawongkul"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A space that is a hybrid space i.e it has both public and private components - such as by having part of it be a private space or which is operated privately</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-04-07</dct:created>
    <skos:prefLabel xml:lang="en">Hybrid Public Private Space</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#PublicSpace"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#PrivateSpace"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#PrivateCommunalSpace">
    <skos:prefLabel xml:lang="en">Private Communal Space</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-04-07</dct:created>
