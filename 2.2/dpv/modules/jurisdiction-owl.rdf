    <sw:term_status xml:lang="en">sunset</sw:term_status>
    <skos:definition xml:lang="en">Location is local and entirely within a virtual environment, such as a software system</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-05-04</dct:modified>
