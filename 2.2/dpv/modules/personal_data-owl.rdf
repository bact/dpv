  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CommerciallyConfidentialData">
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-danielle-welter"/>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-05-30</dct:modified>
