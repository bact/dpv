  <rdf:Description rdf:about="https://w3id.org/dpv/tech/owl#OperatingFactor">
    <skos:prefLabel xml:lang="en">Operating Factor</skos:prefLabel>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
