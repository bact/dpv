    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/tech/owl#"/>
    <skos:definition xml:lang="en">A 'factor' affecting the operation and outcomes of the technology</skos:definition>
    <dct:source xml:lang="en">Model Cards by Mitchell et al. (2019)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
