  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataUser">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-02-14</dct:created>
    <dct:source xml:lang="en">DGA 2.9</dct:source>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
