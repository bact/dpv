    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <skos:prefLabel xml:lang="en">Emotion Recognition</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/ai/owl#EmotionRecognition"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#BiometricVerification">
    <dct:source xml:lang="en">AIA Art.3 (36)</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/tech/owl#Capability"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
