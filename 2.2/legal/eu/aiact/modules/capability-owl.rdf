    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-07-30</dct:modified>
    <skos:related rdf:resource="https://w3id.org/dpv/pd/owl#Biometric"/>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <skos:prefLabel xml:lang="en">Biometric Verification</skos:prefLabel>
    <skos:definition xml:lang="en">The automated, one-to-one verification, including authentication, of the identity of natural persons by comparing their biometric data to previously provided biometric data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#IdentityVerification"/>
  </rdf:Description>
