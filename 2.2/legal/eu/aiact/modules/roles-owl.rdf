    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-07-30</dct:modified>
    <skos:prefLabel xml:lang="en">AI Provider</skos:prefLabel>
    <skos:definition xml:lang="en">A natural or legal person, public authority, agency or other body that develops an AI system or a general-purpose AI model or that has an AI system or a general-purpose AI model developed and places it on the market or puts the AI system into service under its own name or trademark, whether for payment or free of charge</skos:definition>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#NotifyingAuthority">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#NationalCompetentAuthority"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <skos:prefLabel xml:lang="en">Notifying Authority</skos:prefLabel>
    <skos:definition xml:lang="en">The national authority responsible for setting up and carrying out the necessary procedures for the assessment, designation and notification of conformity assessment bodies and for their monitoring</skos:definition>
    <dct:source xml:lang="en">AIA(Art. 3(19))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
