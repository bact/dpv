    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A natural or legal person, public authority, agency or other body using an AI system under its authority except where the AI system is used in the course of a personal non-professional activity</skos:definition>
    <dct:source xml:lang="en">AIA (Art. 3(4))</dct:source>
    <sw:term_status xml:lang="en">modified</sw:term_status>
