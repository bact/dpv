  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA33RiskMitigated">
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <dct:source xml:lang="en">GDPR Article33(3b)</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Justification A33 - Risk Mitigated</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">Justification that the personal data breach was not communicated to the data subject as the controller has taken subsequent measures which ensure that the high risk to the rights and freedoms of data subjects is no longer likely to materialise</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataBreachJustification"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#RiskMitigated"/>
  </rdf:Description>
