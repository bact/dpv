    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A data breach involving cross-border data subjects or processing operations</skos:definition>
    <skos:prefLabel xml:lang="en">Cross-Border Data Breach</skos:prefLabel>
