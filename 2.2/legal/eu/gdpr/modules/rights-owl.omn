        skos:prefLabel "A22 Right to human intervention"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/legal/eu/gdpr/owl#A22>
    
    
Class: <https://w3id.org/dpv/legal/eu/gdpr/owl#A22-3-b>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/gdpr/owl#>,
        skos:definition "Right of the data subject to express his or her point of view"@en,
        skos:prefLabel "A22 Right to express point of view"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/legal/eu/gdpr/owl#A22>
    
    
Class: <https://w3id.org/dpv/legal/eu/gdpr/owl#A22-3-c>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/gdpr/owl#>,
        skos:definition "Right of the data subject to contest the decision"@en,
        skos:prefLabel "A22 Right to contest decision"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/legal/eu/gdpr/owl#A22>
    
    
Class: <https://w3id.org/dpv/legal/eu/gdpr/owl#A22>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/gdpr/owl#>,
        skos:definition "Right not to be subject to a decision based solely on automated processing including profiling, and for the data subject to obtain human intervention on the part of the controller for the contested or objected activity, and for the data subject to express his or her point of view, and for the data subject to contest the decision"@en,
        skos:prefLabel "A22 Right to object to automated decision making"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/gdpr/owl#A7-3>

