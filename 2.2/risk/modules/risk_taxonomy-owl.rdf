  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ConfoundingVariablesBias">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <dct:contributor rdf:nodeID="person-daniel-doherty"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
