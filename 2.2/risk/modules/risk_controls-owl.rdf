  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#SubstitutionControl">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <skos:definition xml:lang="en">Control that substitutes an event with another such that the initial event no longer takes place while the substituted event takes place in place of it</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Substitution Control</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Substitution implies that the replacement event is less risky or is more safe than the replaced event, which is distinct from elimination where no event occurs and thus there is an elimination of risk entirely</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
