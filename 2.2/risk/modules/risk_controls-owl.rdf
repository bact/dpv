    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/vocabulary"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#HaltImpact">
    <skos:definition xml:lang="en">Control that halts the (ongoing) impact event or process such that it no longer takes place or is applicable in the context</skos:definition>
