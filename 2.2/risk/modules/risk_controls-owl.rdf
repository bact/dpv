  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#IdentificationControl">
    <skos:prefLabel xml:lang="en">Identification Control</skos:prefLabel>
    <skos:definition xml:lang="en">Control that identifies the characteristics of an event</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
