  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ReactiveControl">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <skos:prefLabel xml:lang="en">Reactive Control</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Control that is established or functions after an event occurs</skos:definition>
    <skos:scopeNote xml:lang="en">The use of 'reactive' here refers to this control being established i.e. becoming active to address the effects of an event after they occur. It does not imply that the controls are put in place after the event or that there is no planned procedure for handling an incident</skos:scopeNote>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
  </rdf:Description>
