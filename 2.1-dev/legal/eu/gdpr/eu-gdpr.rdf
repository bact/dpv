    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#entities-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A6-1-f-controller">
