    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegitimateInterestOfController"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#Consent">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <skos:prefLabel xml:lang="en">Consent</skos:prefLabel>
    <skos:definition xml:lang="en">Consent’ of the data subject means any freely given, specific, informed and unambiguous indication of the data subject’s wishes by which he or she, by a statement or by a clear affirmative action, signifies agreement to the processing of personal data relating to him or her</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:source rdf:nodeID="N4822445ea9dd486cab4633289ffbdd3a"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Consent"/>
  </rdf:Description>
