  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A49-1-g">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LegalBasis"/>
    <skos:prefLabel xml:lang="en">Art 49(1-g) public register</skos:prefLabel>
    <skos:definition xml:lang="en">The transfer is made from a register which according to Union or Member State law is intended to provide information to the public in general or by any person who can demonstrate a legitimate interest, but only to the extent that the conditions laid down by Union or Member State law for consultation are fulfilled in the particular case.</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#DataTransferLegalBasis"/>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has not Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <dct:source rdf:nodeID="N11149cd300f0464ab7ff7a77bcfec6f7"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#legal-basis-data-transfer-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A49-1-c">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LegalBasis"/>
    <skos:prefLabel xml:lang="en">Art 49(1-c) conclusion of contract</skos:prefLabel>
    <skos:definition xml:lang="en">The transfer is necessary for the conclusion or performance of a contract concluded in the interest of the data subject and controller and another natural or legal person.</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#DataTransferLegalBasis"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Contract"/>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
