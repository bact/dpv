    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#legal-basis-data-transfer-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A49-1-e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
