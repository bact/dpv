  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#Recipient">
    <skos:prefLabel xml:lang="en">Recipient</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Recipient"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">‘Recipient’ means a natural or legal person, public authority, agency or another body, to which the personal data are disclosed, whether a third party or not</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
