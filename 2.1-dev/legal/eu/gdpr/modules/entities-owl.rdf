  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#Processor">
    <dct:source xml:lang="en">GDPR Art.4-8</dct:source>
    <skos:definition xml:lang="en">‘Processor’ means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
