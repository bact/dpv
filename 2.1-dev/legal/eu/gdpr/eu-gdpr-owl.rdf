  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A46-2-f">
    <skos:prefLabel xml:lang="en">Art 46(2-f) certification</skos:prefLabel>
    <skos:definition xml:lang="en">An approved certification mechanism pursuant to GDPR Article 42 together with binding and enforceable commitments of the controller or processor in the third country to apply the appropriate safeguards, including as regards individuals` rights</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
