  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA12IdentityRequired">
    <skos:prefLabel xml:lang="en">Justification A12 - Identity Required</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <dct:source xml:lang="en">GDPR Art.12-6</dct:source>
    <skos:definition xml:lang="en">Justification that a request under A14-21 could not be fulfiled due to lack of identity verification, and therefore requires additional information to complete the identity verification request</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">If the purpose of this justification is to ask for identity verification, then it requires information on what information is considered as an accepable form of identity, which can ideally be expressed through dpv:Process and relevant dpv:PersonalData categories, or through a comment or description for the justification</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#IdentityVerificationRequired"/>
  </rdf:Description>
