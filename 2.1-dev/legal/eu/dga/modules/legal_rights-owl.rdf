    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">The right is scoped to legally binding decisions referred to in Article 14 taken by the competent authorities for data intermediation services in the management, control and enforcement of the notification regime for data intermediation services providers and legally binding decisions referred to in Articles 19 and 24 taken by the competent authorities for the registration of data altruism organisations in the monitoring of recognised data altruism organisations</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <skos:prefLabel xml:lang="en">A28 Right to an effective judicial remedy</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Right of affected natural and legal persons to an effective judicial remedy</skos:definition>
    <dct:source xml:lang="en">DGA 28</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Right"/>
  </rdf:Description>
