  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#CriticalInfrastructure">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Sector"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:prefLabel xml:lang="en">Critical Infrastructure</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">AIA(Art. 3(44h))</dct:source>
    <skos:definition xml:lang="en">An asset, a facility, equipment, a network or a system, or a part of thereof, which is necessary for the provision of an essential service within the meaning of Article 2(4) of Directive (EU) 2022/2557</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Sector"/>
  </rdf:Description>
