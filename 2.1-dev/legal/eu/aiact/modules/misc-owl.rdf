  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#ReasonablyForeseeableMisuse">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#RiskConcept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:scopeNote xml:lang="en">Misuse should be provided through the Risk extension, along with taxonomy e.g. Misuse -&gt; Foreseeable -&gt; Reasonably and NonReasonably Foreseeable; and Misuse -&gt; Unforeseeable Misuse.</skos:scopeNote>
    <dct:source xml:lang="en">AIA (Art. 3(13))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">The use of an AI system in a way that is not in accordance with its intended purpose, but which may result from reasonably foreseeable human behaviour or interaction with other systems, including other AI systems</skos:definition>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:prefLabel xml:lang="en">Reasonably Foreseeable Misuse</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#Misuse"/>
  </rdf:Description>
