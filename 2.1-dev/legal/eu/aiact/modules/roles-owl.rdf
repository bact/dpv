    <dct:title>EU AI Act - Turtle serialiation</dct:title>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIDeployer">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Deployer"/>
    <dct:source xml:lang="en">AIA (Art. 3(4))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:definition xml:lang="en">Any natural or legal person, public authority, agency or other body using an AI system under its authority except where the AI system is used in the course of a personal non-professional activity</skos:definition>
    <skos:prefLabel xml:lang="en">AI Deployer</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Operator"/>
    <skos:definition xml:lang="en">The provider, the product manufacturer, the deployer, the authorised representative, the importer or the distributor;</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:source xml:lang="en">AIA (Art. 3(8))</dct:source>
    <skos:prefLabel xml:lang="en">AI Operator</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#MarketSurveillanceAuthority">
    <skos:definition xml:lang="en">The national authority carrying out the activities and taking the measures pursuant to Regulation (EU) 2019/1020</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA(Art. 3(26))</dct:source>
    <skos:prefLabel xml:lang="en">Market Surveillance Authority</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#NationalCompetentAuthority"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AuthorisedRepresentative">
    <dct:source xml:lang="en">AIA (Art. 3(5))</dct:source>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Representative"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union who has received and accepted a written mandate from a provider of an AI system or a general-purpose AI model to, respectively, perform and carry out on its behalf the obligations and procedures established by this Regulation</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:prefLabel xml:lang="en">Authorised Representative</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#NotifiedBody">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#ConformityAssessmentBody"/>
    <skos:prefLabel xml:lang="en">Notified Body</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA(Art. 3(22))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:definition xml:lang="en">A conformity assessment body notified in accordance with this Regulation and other relevant Union harmonisation legislation</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#ConformityAssessmentBody">
    <skos:prefLabel xml:lang="en">Conformity Assessment Body</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Auditor"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A body that performs third-party conformity assessment activities, including testing, certification and inspection</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:source xml:lang="en">AIA(Art. 3(21))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/guides">
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/guidance"/>
    <dct:title>Guides for Data Privacy Vocabulary</dct:title>
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
    <dct:conformsTo rdf:resource="https://www.w3.org/TR/html/"/>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/html"/>
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/guides"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIImporter">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Importer"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:source xml:lang="en">AIA (Art. 3(6))</dct:source>
    <skos:prefLabel xml:lang="en">AI Importer</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union that places on the market an AI system that bears the name or trademark of a natural or legal person established outside the Union</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/examples">
    <dct:conformsTo rdf:resource="https://www.w3.org/TR/html/"/>
    <dct:title>Examples for Data Privacy Vocabulary</dct:title>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/html"/>
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/examples"/>
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/guidance"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#NationalCompetentAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">The notifying authority and the market surveillance authority. As regards AI systems put into service or used by EU institutions, agencies, offices and bodies, any reference to national competent authorities or market surveillance authorities in this Regulation shall be understood as referring to the European Data Protection Supervisor.</skos:definition>
    <dct:source xml:lang="en">AIA(Art. 3(42))</dct:source>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:prefLabel xml:lang="en">National Competent Authority</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Authority"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIDistributor">
    <skos:definition xml:lang="en">Any natural or legal person in the supply chain, other than the provider or the importer, that makes an AI system available on the Union market</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Distributor"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">AI Distributor</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:source xml:lang="en">AIA (Art. 3(7))</dct:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/primer">
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/html"/>
    <dct:conformsTo rdf:resource="https://www.w3.org/TR/html/"/>
    <dct:title>Primer for Data Privacy Vocabulary</dct:title>
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/primer"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/guidance"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIOffice">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:source xml:lang="en">AIA(Art. 3(42))</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Authority"/>
    <skos:definition xml:lang="en">Commission’s function of contributing to the implementation, monitoring and supervision of AI systems and AI governance. References in this Regulation to the Artificial Intelligence office shall be understood as references to the Commission.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:prefLabel xml:lang="en">Artificial Intelligence Office</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#NotifyingAuthority">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#NationalCompetentAuthority"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Notifying Authority</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">The national authority responsible for setting up and carrying out the necessary procedures for the assessment, designation and notification of conformity assessment bodies and for their monitoring</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">AIA(Art. 3(19))</dct:source>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#DownstreamAIProvider">
    <skos:definition xml:lang="en">A provider of an AI system, including a general-purpose AI system, which integrates an AI model, regardless of whether the model is provided by themselves and vertically integrated or provided by another entity based on contractual relations.</skos:definition>
