  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#PostRemoteBiometriIdentificationSystem">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:source xml:lang="en">AIA(Art. 3(38))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:related rdf:resource="https://w3id.org/dpv/pd/owl#Biometric"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:definition xml:lang="en">A remote biometric identification system other than a ‘real-time’ remote biometric identification system</skos:definition>
    <skos:prefLabel xml:lang="en">Post Remote Biometric Identification System</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#RemoteBiometricIdentificationSystem"/>
  </rdf:Description>
