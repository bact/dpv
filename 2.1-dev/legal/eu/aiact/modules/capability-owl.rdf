  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#BiometricIdentification">
    <skos:prefLabel xml:lang="en">Biometric Identification</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/tech/owl#Capability"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AI Act Art.3-33a</dct:source>
    <skos:related rdf:resource="https://w3id.org/dpv/pd/owl#Biometric"/>
    <skos:definition xml:lang="en">Capability where the automated recognition of physical, physiological, behavioural, and psychological human features are used for the purpose of establishing an individual’s identity by comparing biometric data of that individual to stored biometric data of individuals in a database</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
