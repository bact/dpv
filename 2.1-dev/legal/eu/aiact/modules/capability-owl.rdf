  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#BiometricCategorisation">
    <skos:definition xml:lang="en">Assigning natural persons to specific categories on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Capability"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">based on AIA(Art. 3(35))</dct:source>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:related rdf:resource="https://w3id.org/dpv/pd/owl#Biometric"/>
    <skos:prefLabel xml:lang="en">Biometric Categorisation</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#BiometricIdentification">
    <skos:prefLabel xml:lang="en">Biometric Identification</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA(Art. 3(33a))</dct:source>
    <skos:related rdf:resource="https://w3id.org/dpv/pd/owl#Biometric"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:definition xml:lang="en">The automated recognition of physical, physiological, behavioural, and psychological human features for the purpose of establishing an individualâ€™s identity by comparing biometric data of that individual to stored biometric data of individuals in a database</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Capability"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
  </rdf:Description>
