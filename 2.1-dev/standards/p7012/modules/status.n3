p7012:PrivacyNegotiationClarificationRequired a rdfs:Class,
        skos:Concept,
        p7012:PrivacyNegotiationStatus ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:broader p7012:PrivacyNegotiationStatus ;
    skos:definition "Status indicating the Privacy Negotiation requires a clarification from the other party regarding the relevant context"@en ;
    skos:inScheme p7012:status-classes ;
    skos:prefLabel "Privacy Negotiation Clarification Required"@en .

p7012:PrivacyNegotiationConfirmationRequired a rdfs:Class,
        skos:Concept,
