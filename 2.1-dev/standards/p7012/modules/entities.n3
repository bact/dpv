    rdfs:subClassOf p7012:PartyAgent ;
    sw:term_status "accepted"@en ;
    skos:broader p7012:PartyAgent ;
    skos:definition "A Privacy Agent that repersents the Service Provider or Second Party"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Provider Agent"@en .

p7012:ResponsibleParty a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf p7012:Party ;
    sw:term_status "accepted"@en ;
    skos:broader p7012:Party ;
    skos:definition "The party that is responsible for a specific activity"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Responsible Party"@en .

p7012:ServiceProvider a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf dpv:ServiceConsumer,
        p7012:Party ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:ServiceConsumer,
        p7012:Party ;
    skos:definition "The Service Provider involved as the Second Party in the contractual interaction"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Service Provider"@en .

p7012:ThirdParty a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf dpv:ThirdParty,
        p7012:Party ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:ThirdParty,
        p7012:Party ;
    skos:definition "A party other than First and Second Party in the contractual interaction"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Third Party"@en .

