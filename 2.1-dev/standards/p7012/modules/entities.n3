p7012:Authority a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf dpv:Authority,
        p7012:Party ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Authority,
        p7012:Party ;
    skos:definition "A legal authority that is empowered to investigate and determine compliance"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Authority"@en .

p7012:Party a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf dpv:LegalEntity ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:LegalEntity ;
    skos:definition "An entity that is part of a contractual interaction and has decision making abilities or has obligations and responsibilities"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Party"@en .

p7012:PartyAgent a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf dpv:Entity ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Entity ;
    skos:definition "An agent that represents a party and can act on behalf of it - such as for signing contracts or negotiating terms"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Party Agent"@en ;
    skos:scopeNote "Agents can be human or non-human i.e. machine agents"@en .

p7012:Person a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf dpv:NaturalPerson,
        p7012:Party ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:NaturalPerson,
        p7012:Party ;
    skos:definition "The human individual or person involved as the First Party in the contractual interaction"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Person"@en .

p7012:PersonAgent a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf p7012:PartyAgent ;
    sw:term_status "accepted"@en ;
    skos:broader p7012:PartyAgent ;
    skos:definition "A Privacy Agent that represents the Person or First Party"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Person Agent"@en .

p7012:PrivacyAgent a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    rdfs:subClassOf p7012:PartyAgent ;
    sw:term_status "accepted"@en ;
    skos:broader p7012:PartyAgent ;
    skos:definition "An agent that represents a party and can act on behalf of it to make decisions regarding privacy (and related matters)"@en ;
    skos:inScheme p7012:entities-classes ;
    skos:prefLabel "Privacy Agent"@en ;
    skos:scopeNote "Privacy Agents can be human or non-human i.e. machine agents"@en .

p7012:ProviderAgent a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
