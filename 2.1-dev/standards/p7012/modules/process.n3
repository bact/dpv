p7012:PrivacyEndpoint a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "An endpoint, such as a URL or an address, where privacy related communications can be made"@en ;
    skos:inScheme p7012:process-classes ;
    skos:prefLabel "Privacy Endpoint"@en .

p7012:PrivacyNegotiation a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "A negotiation between parties regarding choosing the appropriate privacy terms"@en ;
    skos:inScheme p7012:process-classes ;
    skos:prefLabel "Privacy Negotiation"@en .

p7012:PrivacyNegotiationRecord a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "A record of a privacy negotiation, including both accepted and rejected negotiations as well as the creation of contracts"@en ;
    skos:inScheme p7012:process-classes ;
    skos:prefLabel "Privacy Negotiation Record"@en .

p7012:PrivacyNegotiationRequest a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "A request to initiate a privacy negotiation"@en ;
    skos:inScheme p7012:process-classes ;
    skos:prefLabel "Privacy Negotiation Request"@en .

p7012:PrivacyPreference a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "A preference regarding privacy term(s) which indicates the terms are preferred over other terms, or are acceptable, or are unacceptable"@en ;
    skos:inScheme p7012:process-classes ;
    skos:prefLabel "Privacy Preference"@en .

p7012:PrivacyTerm a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "A (contractual) term regarding privacy that expresses permissions, prohibitions, and obligations"@en ;
    skos:inScheme p7012:process-classes ;
    skos:prefLabel "Privacy Term"@en .

p7012:PrivacyTermRegistry a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "A registry of privacy terms"@en ;
    skos:inScheme p7012:process-classes ;
    skos:prefLabel "Privacy Term Registry"@en .

