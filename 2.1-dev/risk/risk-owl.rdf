  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#EliminationControl">
    <skos:definition xml:lang="en">Control that eliminates an event entirely such that the event does not occur</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
