    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:prefLabel xml:lang="en">Group Attribution Bias</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#CognitiveBias"/>
  </rdf:Description>
