    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#PotentialRisk"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:prefLabel xml:lang="en">Out-Group Homogeneity Bias</skos:prefLabel>
    <skos:definition xml:lang="en">Bias that occurs when seeing out-group members as more alike than in-group members when comparing attitudes, values, personality traits, and other characteristics</skos:definition>
    <dct:contributor>Daniel Doherty, Delaram Golpayegani, Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-09-13</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">ISO/IEC 24027:2021</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#CognitiveBias"/>
  </rdf:Description>
