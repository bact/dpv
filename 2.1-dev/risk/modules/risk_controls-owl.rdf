  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ReductionControl">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <skos:prefLabel xml:lang="en">Reduction Control</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Control that reduces the effects of an event</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <skos:scopeNote xml:lang="en">Reduction here refers to a lessening of the effects after the event has occured by either reducing their likelihood or their severity in the context. This can involve changing the underlying context such that the effects have a reduced chance of occuring, or to create additional measures such that the effects do not have the initial severity for an entity</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ReactiveControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#RemoveImpact">
