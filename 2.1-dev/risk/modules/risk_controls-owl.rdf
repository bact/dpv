  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#RecoveryControl">
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:prefLabel xml:lang="en">Recovery Control</skos:prefLabel>
    <skos:definition xml:lang="en">Control that aims to restore the context following an event</skos:definition>
    <dct:contributor>Delaram Golpayegani, Harshvardhan J. Pandit</dct:contributor>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:modified>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <skos:scopeNote xml:lang="en">Recovery implies taking steps to correct the effects which may not be the same as the initial conditions before the event, whereas risk:ReversalControl refers to undoing the effects such that the initial condition is restored</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ResolutionControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#AvoidSource">
