    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ConsequenceControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#IdentificationControl">
    <skos:prefLabel xml:lang="en">Identification Control</skos:prefLabel>
    <skos:definition xml:lang="en">Control that identifies the characteristics of an event</skos:definition>
    <dct:contributor>Delaram Golpayegani, Harshvardhan J. Pandit</dct:contributor>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <skos:scopeNote xml:lang="en">Identification in the context of an event refers to its characteristics such as likelihood, severity, as well as contextual metrics such as amount of data or power being used, or affected entities or things, and which can be used to categorise the event in terms of risk level or other contextual groupings</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#MonitorControl"/>
  </rdf:Description>
