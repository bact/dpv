    <skos:prefLabel xml:lang="en">Avoid Source</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#SourceControl"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#AvoidanceControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#RemoveConsequence">
