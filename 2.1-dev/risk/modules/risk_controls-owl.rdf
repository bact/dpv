  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ModificationControl">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <dct:contributor rdf:nodeID="N408f8be2f1e34aa5ba52aa205e7a7c4b"/>
    <dct:contributor rdf:nodeID="N9502991e21554b8d8a638a007ebecaa2"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Modification necessitates a change in the context to result in a change in the event's characteristics. Where such changes reduce the likelihood or effects of the event, the modification also works as a mitigation for the event</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Modification Control</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:definition xml:lang="en">Control that modifies the context to change the event's characteristics such that the event still occurs but with the modified characteristics with the goal of managing an event that is accepted to occur</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ProactiveControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#OverrideControl">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Control that aims to override the event with the goal of avoiding its further effects</skos:definition>
