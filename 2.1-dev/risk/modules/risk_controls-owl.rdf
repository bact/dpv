    <skos:definition xml:lang="en">Control that halts the (ongoing) risk source event or process such that it no longer takes place or is applicable in the context</skos:definition>
    <skos:prefLabel xml:lang="en">Halt Source</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#SourceControl"/>
