    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Control that proactively removes the impact event such that the event does not occur in the context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <skos:prefLabel xml:lang="en">Remove Impact</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#EliminationControl"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ImpactControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ModificationControl">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Modification Control</skos:prefLabel>
