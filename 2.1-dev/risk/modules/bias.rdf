  <rdf:Description rdf:about="https://w3id.org/dpv/risk#NonNormalityBias">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk#RiskConcept"/>
    <skos:prefLabel xml:lang="en">Non-Normality Bias</skos:prefLabel>
    <skos:definition xml:lang="en">Bias that occurs when the dataset is subject to a different (i.e. non-normal) distribution (e.g., Chi-Square, Beta, Lorentz, Cauchy, Weibull or Pareto) where the results can be biased and misleading</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/risk#StatisticalBias"/>
    <dct:source xml:lang="en">ISO/IEC 24027:2021</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-09-13</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Daniel Doherty</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/risk#bias-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk#serialisation-n3">
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
