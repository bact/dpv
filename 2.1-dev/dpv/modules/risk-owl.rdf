    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0068"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Impact"/>
    <skos:definition xml:lang="en">Indicates impact(s) possible or arising as consequences from specified concept</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-05-18</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Impact"/>
    <skos:prefLabel xml:lang="en">has impact</skos:prefLabel>
  </rdf:Description>
