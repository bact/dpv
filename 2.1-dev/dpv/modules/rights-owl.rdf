  <rdf:Description rdf:about="http://purl.org/dc/terms/accessRights">
    <skos:prefLabel xml:lang="en">dct:accessRights</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Also used for specifying constraints on access associated with Rights Exercising (e.g. User must log in) or access to provided data (e.g. access via link)</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
