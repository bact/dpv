    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/vocabulary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/valid">
    <skos:scopeNote xml:lang="en">Also used for specifying the temporal validity of an activity associated with Right Exercise. For example, limits on duration for providing or accessing provided information</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">dct:valid</skos:prefLabel>
  </rdf:Description>
