  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isExercisedAt">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">is exercised at</skos:prefLabel>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#ActiveRight"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseNotice"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseNotice"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:definition xml:lang="en">Indicates context or information about exercising a right</skos:definition>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#ActiveRight"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedByEntity">
