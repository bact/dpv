  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isAfter">
    <skos:scopeNote xml:lang="en">Also used for specifying a RightExerciseActivity occurs before another RightExerciseActivity</skos:scopeNote>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseActivity"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
