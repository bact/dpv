    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseActivity"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Recipient"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/isPartOf">
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseRecord"/>
