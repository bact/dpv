    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0085"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0037"/>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseActivity"/>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseActivity"/>
    <skos:scopeNote xml:lang="en">Also used to indicate the Entity that implements or performs a Right Exercise Activity</skos:scopeNote>
