    sw:term_status "accepted"@en ;
    skos:broader dpv:ContractAccepted ;
    skos:definition "Status indicating the contract has been accepted by the specific entity in context"@en ;
    skos:inScheme dpv:contract-status-classes ;
    skos:prefLabel "Contract Accepted by This Entity"@en .

