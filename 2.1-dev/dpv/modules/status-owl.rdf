    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Status indicating the specified context was expected</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ExpectationStatus"/>
  </rdf:Description>
