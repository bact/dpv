    <skos:scopeNote xml:lang="en">This concept is the same as "Data Mapping" and "Data Flow Mapping Exercise" used by the Information Comissioner's Office as "to document the data that flows in, around, and out of information processing systems or services"</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#InformationAudit"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Notification">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
