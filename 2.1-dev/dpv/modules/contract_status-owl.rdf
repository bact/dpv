    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Status representing the drafting of contract text has been completed and it can now be offered for signing</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractPreparationStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Contract Drafted</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-09</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractPreparationStatus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractNotFulfiled">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractFulfilmentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Status representing contract where none of its terms have been fulfiled in a manner that does not constitutea violation or breach of the contract i.e. there is still time and opportunity to complete the terms</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-09</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <skos:prefLabel xml:lang="en">Contract Not Fulfiled</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractFulfilmentStatus"/>
  </rdf:Description>
