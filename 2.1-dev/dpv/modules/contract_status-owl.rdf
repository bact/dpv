  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractPartiallySigned">
    <skos:definition xml:lang="en">Status representing contract has been partially signed by parties i.e. some parties have signed the contract and others are yet to make a decision to sign it</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractExecutionStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Contract Partially Signed</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-09</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractExecutionStatus"/>
  </rdf:Description>
