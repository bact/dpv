  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractAccepted">
    <skos:scopeNote xml:lang="en">Additional specialised concepts are provided and recommended to represent the common but contextual uses of the phrasing "contract accepted" for 1) indicate this party has accepted the contract - dpv:ContractAcceptedThisEntity; 2) indicate some parties have accepted it and others are yet to make a decision - dpv:ContractAcceptedSomeEntities; and 3) indicate all parties have accepted the contract - dpv:ContractAcceptedAllEntities</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-08-27</dct:created>
    <skos:definition xml:lang="en">Status indicating the contract has been accepted by some or all entities</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Contract Accepted</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractStatus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractedAcceptedAllEntities">
