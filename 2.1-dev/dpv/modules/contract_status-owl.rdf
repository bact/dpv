  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractSignedByParty">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractExecutionStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Status representing contract has been signed by the indicated signing party</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-09</dct:created>
    <skos:scopeNote xml:lang="en">The signing party can be expressed using dpv:hasParty with the status along with metadata such as a timestamp, or the status can be used to list whether each party in the contract has signed or not e.g. it is reviewing the contract</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Contract Signed By Party</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractExecutionStatus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractViolated">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractFulfilmentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-09</dct:created>
    <skos:prefLabel xml:lang="en">Contract Violated</skos:prefLabel>
    <skos:definition xml:lang="en">Status representing contract where one or more terms have not been fulfiled or have been fulfiled, where either is considered a violation of the terms</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractFulfilmentStatus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractRenewed">
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-09</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractPerformanceStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Status representing contract being renewed with new duration and/or applicability where the contract has been fully executed in the past</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Contract Renewed</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractPerformanceStatus"/>
  </rdf:Description>
