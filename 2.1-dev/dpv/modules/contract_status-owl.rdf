  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractActive">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractActivationStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">A contract being active only refers to its terms now being required to be fulfiled, while dpv:ContractBeingPerformed states that they are being performed</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Status representing contract that has been fully executed and whose terms are considered active i.e. they are applicable and are required to be performed</skos:definition>
    <skos:prefLabel xml:lang="en">Contract Active</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-09</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractActivationStatus"/>
  </rdf:Description>
