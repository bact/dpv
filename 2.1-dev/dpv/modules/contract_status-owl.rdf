  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractOffered">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Contract Offered</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ContractPreparationStatus"/>
