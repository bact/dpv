  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractFullySigned">
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Julian Flake</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
