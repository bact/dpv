  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ContractActivationStatus">
    <skos:prefLabel xml:lang="en">Contract Activation Status</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
