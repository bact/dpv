  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataImporter">
    <dct:source rdf:nodeID="N3c4e88104a004336a9a2a284717c3feb"/>
    <dct:contributor rdf:nodeID="N0ad0350864284a3ab9346bfd88cd0246"/>
