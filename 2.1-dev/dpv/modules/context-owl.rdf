  <rdf:Description rdf:about="https://w3id.org/dpv/owl#SingularFrequency">
    <skos:prefLabel xml:lang="en">Singular Frequency</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Frequency"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
