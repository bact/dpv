  <rdf:Description rdf:about="https://w3id.org/dpv/owl#NotRequired">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Necessity"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-15</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Paul Ryan, Georg P. Krog, Julian Flake, Beatriz Esteves</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
