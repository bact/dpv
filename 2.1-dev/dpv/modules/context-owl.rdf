    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/primer"/>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/html"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isNotApplicableFor">
    <skos:definition xml:lang="en">Indicates the concept or information is not applicable for specified context</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Scope"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit, Beatriz Esteves, Georg P. Krog, Paul Ryan</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-13</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
