    <skos:definition xml:lang="en">Indicates situations where the context is not applicable, information is not available, or this is unknown. An appropriate instance of dpv:Applicability should be used with this relation to express the situation</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Applicability"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
