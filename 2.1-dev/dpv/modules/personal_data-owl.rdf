    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ObservedData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#CollectedPersonalData"/>
    <dct:contributor rdf:nodeID="Nf8624e206b01445e92dcf177afedac67"/>
    <skos:prefLabel xml:lang="en">Observed Personal Data</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
