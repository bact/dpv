  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CollectedPersonalData">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#PersonalData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#CollectedData"/>
    <skos:definition xml:lang="en">Personal Data that has been collected from another source such as the Data Subject</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <skos:prefLabel xml:lang="en">Collected Personal Data</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0046"/>
    <skos:scopeNote xml:lang="en">To indicate the source of data, use the DataSource concept with the hasDataSource relation</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-30</dct:created>
    <dct:contributor rdf:nodeID="N0f5cd4383a874823a56bca9ac63a903c"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#PersonalData">
    <skos:prefLabel xml:lang="en">Personal Data</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Data"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/langs/usage-policy#AnyData"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0044"/>
    <skos:definition xml:lang="en">Data directly or indirectly associated or related to an individual.</skos:definition>
    <dct:source rdf:nodeID="N3b90a91e0b0148b299bae5d8100d7389"/>
    <skos:scopeNote xml:lang="en">This definition of personal data encompasses the concepts used in GDPR Art.4-1 for 'personal data' and ISO/IEC 2700 for 'personally identifiable information (PII)'.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:contributor rdf:nodeID="N0f5cd4383a874823a56bca9ac63a903c"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
