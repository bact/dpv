  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RenewedConsentGiven">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:source rdf:nodeID="Ne780d14b1478483591e2928512aa9d63"/>
