  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedByEntity">
    <skos:definition xml:lang="en">Indicates implementation details such as entities or agents</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
