  <rdf:Description rdf:about="https://w3id.org/dpv/owl#NonPersonalDataProcess">
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-09</dct:created>
    <skos:scopeNote xml:lang="en">Use of personal data within NonPersonalDataProcess should be considered a violation of the explicit constraint that no personal data is involved.</skos:scopeNote>
    <skos:definition xml:lang="en">An action, activity, or method involving non-personal data, and asserting that no personal data is involved</skos:definition>
