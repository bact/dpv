    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Data Exporter</skos:prefLabel>
    <dct:contributor>David Hickey, Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
