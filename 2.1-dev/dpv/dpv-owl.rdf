  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ExpressedConsent">
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0018"/>
    <skos:scopeNote xml:lang="en">Expressed consent requires the individual take a specific and unambiguous action that directly indicates their consent. This action may be a part of other processes such as setting preferences, or agreeing to a contract, or other matters not relating to consent. An example of expressed consent is interacting with a checkbox within a dashboard or clicking a button on a web form</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-21</dct:created>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Paul Ryan, Julian Flake</dct:contributor>
    <skos:prefLabel xml:lang="en">Expressed Consent</skos:prefLabel>
    <skos:definition xml:lang="en">Consent that is expressed through an action intended to convey a consenting decision</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#InformedConsent"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Pseudonymisation">
    <skos:prefLabel xml:lang="en">Pseudonymisation</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar, Maya Borges, Damien Desfontaines</dct:contributor>
    <skos:definition xml:lang="en">Pseudonymisation means the processing of personal data in such a manner that the personal data can no longer be attributed to a specific data subject without the use of additional information, provided that such additional information is kept separately and is subject to technical and organisational measures to ensure that the personal data are not attributed to an identified or identifiable natural person;</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-24</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:source rdf:nodeID="N4cadacf981ab4378b18ee123a146b33a"/>
