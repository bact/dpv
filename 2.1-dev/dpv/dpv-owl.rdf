  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Purpose">
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0041"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0007"/>
