  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentStatus">
    <skos:definition xml:lang="en">The state or status of 'consent' that provides information reflecting its operational status and validity for processing data</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
