    <skos:prefLabel xml:lang="en">Technique</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">The underlying technological algorithm, method, or process that forms the technique for using or applying AI</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/ai/owl#AI"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
  </rdf:Description>
