  <rdf:Description rdf:about="https://w3id.org/dpv/ai#AutomationBias">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk#RiskConcept"/>
    <skos:prefLabel xml:lang="en">Automation Bias</skos:prefLabel>
    <skos:definition xml:lang="en">Bias tha occurs due to propensity for humans to favour suggestions from automated decision-making systems and to ignore contradictory information made without automation, even if it is correct</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/risk#CognitiveBias"/>
    <dct:source xml:lang="en">ISO/IEC 24027:2021</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-09-13</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Daniel Doherty</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/ai#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/ai#bias-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/ai#EngineeringDecisionBias">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk#RiskConcept"/>
    <skos:prefLabel xml:lang="en">Engineering Decision Bias</skos:prefLabel>
    <skos:definition xml:lang="en">Bias that occurs due to machine learning model architectures - encompassing all model specifications, parameters and manually designed features</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/ai#AIBias"/>
    <skos:scopeNote xml:lang="en">Data bias and human cognitive bias can contribute to such bias</skos:scopeNote>
    <dct:source xml:lang="en">ISO/IEC 24027:2021</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-09-13</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Daniel Doherty</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/ai#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/ai#bias-classes"/>
  </rdf:Description>
