  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#MitigationControl">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Control that aims to reduce the likelihood or effect of an event with the goal of managing an event accepted to occur</skos:definition>
    <skos:prefLabel xml:lang="en">Mitigation Control</skos:prefLabel>
    <dct:contributor rdf:nodeID="N9c63c24625fe482386a350b4ea27b4f8"/>
    <dct:contributor rdf:nodeID="Ne22a67b17c7e4a779c30d2864b6a0cee"/>
    <skos:scopeNote xml:lang="en">Mitigation requires accepting that an event will occur, and thereby focusing on managing it by reducing its likelihood or effects by adding additional processes to specifically address the event or its effects</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ProactiveControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ReactiveControl">
