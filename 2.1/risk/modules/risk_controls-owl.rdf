    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/risk/owl#AvoidanceControl"/>
    <skos:definition xml:lang="en">Indicates the event or action is avoided by the specified control</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/risk/owl#AvoidanceControl"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:contributor rdf:nodeID="Ne22a67b17c7e4a779c30d2864b6a0cee"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/risk/owl#reduces"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#RiskConcept"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#RiskConcept"/>
  </rdf:Description>
