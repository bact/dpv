  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentRefused">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor rdf:nodeID="Nb0b5fba741814592ba04e1a368a761fa"/>
    <dct:contributor rdf:nodeID="N2fe5586a82794e5a99ff2b47c1f2175e"/>
    <dct:contributor rdf:nodeID="N3f795f08d5094633824a00c38b4e8326"/>
    <dct:contributor rdf:nodeID="N2acb95e70d08440bb095f34b511771ce"/>
    <skos:prefLabel xml:lang="en">Consent Refused</skos:prefLabel>
    <skos:definition xml:lang="en">The state where consent has been refused</skos:definition>
    <skos:scopeNote xml:lang="en">An example of this state is when the individual clicks on a 'disagree' or 'reject' or 'refuse' button, or leaves a checkbox unticked</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <dct:source rdf:nodeID="N2cb91bd24e9740dcafc86542752dc331"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ConsentStatusInvalidForProcessing"/>
  </rdf:Description>
