  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataExporter">
    <dct:contributor rdf:nodeID="N31e62ba845384be18dfaf6cf87451c37"/>
    <dct:contributor rdf:nodeID="N3f795f08d5094633824a00c38b4e8326"/>
