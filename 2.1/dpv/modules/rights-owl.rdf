    <dct:contributor rdf:nodeID="N18c0395db3e14fc6b2c509914ad20ef9"/>
    <skos:prefLabel xml:lang="en">Right Exercise Activity</skos:prefLabel>
    <skos:definition xml:lang="en">An activity representing an exercising of an active right</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-02</dct:created>
    <skos:scopeNote xml:lang="en">There may be multiple activities associated with exercising and fulfilling rights. See the RightExerciseRecord concept for record-keeping of such activities in a cohesive manner.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0059"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
  </rdf:Description>
