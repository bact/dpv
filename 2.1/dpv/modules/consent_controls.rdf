  <rdf:Description rdf:about="https://w3id.org/dpv#ManageConsent">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Manage Consent</skos:prefLabel>
    <skos:definition xml:lang="en">Control for managing a given consent in terms of providing, reaffirming, or withdrawing it</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#ProvideConsent"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#WithdrawConsent"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#ReaffirmConsent"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#ProvideConsent"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#WithdrawConsent"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#ReaffirmConsent"/>
    <skos:scopeNote xml:lang="en">Indicates how the data subject can manage their consent in terms of providing it, or reaffirming or withdrawing a given consent. This concept is useful to represent a single location or interface where multiple controls are provided for consent</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#consent-controls-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#ConsentControl">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Consent Control</skos:prefLabel>
    <skos:definition xml:lang="en">The control or activity associated with obtaining, providing, withdrawing, or reaffirming consent</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#EntityInvolvement"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#EntityInvolvement"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#consent-controls-classes"/>
  </rdf:Description>
