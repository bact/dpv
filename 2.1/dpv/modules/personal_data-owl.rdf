    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DerivedData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#PersonalData"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0009"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0046"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Personal Data that is obtained or derived from other data</skos:definition>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/vocabs/data#Derived"/>
