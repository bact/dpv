  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA14EntityAlreadyInformed">
    <dct:contributor rdf:nodeID="N75641c194b3f4973ab2abfb33aed55d3"/>
    <dct:contributor rdf:nodeID="Ne22a67b17c7e4a779c30d2864b6a0cee"/>
    <dct:contributor rdf:nodeID="N7d9b5ee7f31f4f2ba5951e30b49ec7c0"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Justification A14 - Entity Already Informed</skos:prefLabel>
    <skos:scopeNote xml:lang="en">This justification requires information on how the data subject was provided the information to satify the assertion, which can be provided as a description of comment</skos:scopeNote>
    <dct:source xml:lang="en">GDPR Art.14-5a</dct:source>
    <skos:definition xml:lang="en">Justification that A14 obligations for providing information do not apply as the data subject already has the information</skos:definition>
