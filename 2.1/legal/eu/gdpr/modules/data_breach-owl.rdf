  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA33BreachedDataIneffective">
    <skos:prefLabel xml:lang="en">Justification A33 - Breached Data Ineffective</skos:prefLabel>
    <dct:contributor rdf:nodeID="N75641c194b3f4973ab2abfb33aed55d3"/>
    <dct:contributor rdf:nodeID="N7d9b5ee7f31f4f2ba5951e30b49ec7c0"/>
