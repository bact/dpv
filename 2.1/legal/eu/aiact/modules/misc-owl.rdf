  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AILiteracy">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor rdf:nodeID="N9c63c24625fe482386a350b4ea27b4f8"/>
    <skos:prefLabel xml:lang="en">AI Literacy</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Skills, knowledge and understanding that allows providers, users and affected persons, taking into account their respective rights and obligations in the context of the AI Act, to make an informed deployment of AI systems, as well as to gain awareness about the opportunities and risks of AI and possible harm it can cause.</skos:definition>
    <dct:source xml:lang="en">AIA (Art. 3(bh))</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AILiteracy"/>
  </rdf:Description>
