    <sw:term_status xml:lang="en">modified</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/ai/owl#EmotionRecognition"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#DeepFakeGeneration">
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:contributor rdf:nodeID="Nf2624642076a4610a51afb2a4c6e9cdd"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:modified>
    <skos:definition xml:lang="en">Capability to generate or manipulate image, audio or video content that resembles existing persons, objects, places or other entities or events and would falsely appear to a person to be authentic or truthful</skos:definition>
    <skos:prefLabel xml:lang="en">Deep Fake Generation</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/tech/owl#Capability"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AI Act Art.3-bl</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/ai/owl#BiometricIdentification"/>
  </rdf:Description>
