  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataIntermediationAuthority">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Authority"/>
    <skos:definition xml:lang="en">An authority tasked with overseeing the activity of data intermediation service providers and maintaining a public register of said entities</skos:definition>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <skos:prefLabel xml:lang="en">Data Intermediation Authority</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
