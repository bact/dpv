    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-02-14</dct:created>
    <skos:definition xml:lang="en">An entity who is responsible for receiving and transmitting requests for the reuse of public data for a particular sector</skos:definition>
    <skos:prefLabel xml:lang="en">Sectorial Single Information Point Provider</skos:prefLabel>
  </rdf:Description>
