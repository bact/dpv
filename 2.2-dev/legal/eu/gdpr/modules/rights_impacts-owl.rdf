    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">A13 Violated</skos:prefLabel>
    <skos:definition xml:lang="en">A violation of A13 obligation regarding providing information</skos:definition>
