  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A13-Violated">
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskConcept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#PotentialConsequence"/>
