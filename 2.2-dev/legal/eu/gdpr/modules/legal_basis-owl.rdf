  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A6-1-a-non-explicit-consent">
    <dct:contributor rdf:nodeID="N5a10be0316db488891b58503e4b98035"/>
    <dct:contributor rdf:nodeID="Nbbef7d4d07c943659aec38002823cece"/>
