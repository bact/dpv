    <skos:definition xml:lang="en">‘International Organisation’ means an organisation and its subordinate bodies governed by public international law, or any other body which is set up by, or on the basis of, an agreement between two or more countries</skos:definition>
    <skos:prefLabel xml:lang="en">International Organisation</skos:prefLabel>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#InternationalOrganisation"/>
