    <dct:source xml:lang="en">GDPR Art.12-3</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#DelayJustification"/>
  </rdf:Description>
