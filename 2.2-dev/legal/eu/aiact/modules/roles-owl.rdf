  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator">
    <skos:prefLabel xml:lang="en">AI Operator</skos:prefLabel>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <skos:definition xml:lang="en">The provider, the product manufacturer, the deployer, the authorised representative, the importer or the distributor;</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:source xml:lang="en">AIA (Art. 3(8))</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Operator"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIImporter">
    <skos:prefLabel xml:lang="en">AI Importer</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Importer"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union that places on the market an AI system that bears the name or trademark of a natural or legal person established outside the Union</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">AIA (Art. 3(6))</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
