  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#BiometricIdentification">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:modified>
    <rdf:type rdf:resource="https://w3id.org/dpv/tech/owl#Capability"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
