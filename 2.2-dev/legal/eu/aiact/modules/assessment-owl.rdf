  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#HighRiskAIAssessment">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:source xml:lang="en">Art. 9 (2b)</dct:source>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">An assessment undertaken to determine whether the AI system is classified as high-risk</skos:definition>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <skos:prefLabel xml:lang="en">High Risk AI Assessment</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Assessment"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#CEMarking">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA(Art. 3(24))</dct:source>
    <skos:prefLabel xml:lang="en">CE Marking</skos:prefLabel>
    <skos:definition xml:lang="en">A marking by which a provider indicates that an AI system is in conformity with the requirements set out in Chapter III, Section 2 of the AI Act and other applicable Union legislation harmonising the conditions for the marketing of products (‘Union harmonisation legislation’) providing for its affixing</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#CertificationSeal"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#ConformityAssessment">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:source xml:lang="en">AIA(Art. 3(20))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
