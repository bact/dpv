  <rdf:Description rdf:about="https://w3id.org/dpv#LocationLocality">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Location Locality</skos:prefLabel>
    <skos:definition xml:lang="en">Locality refers to whether the specified location is local within some context, e.g. for the user</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Location"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-04</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples#E0060"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#FederatedLocations">
