    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LocationFixture"/>
    <skos:prefLabel xml:lang="en">Federated Locations</skos:prefLabel>
    <skos:definition xml:lang="en">Location that is federated across multiple separate areas with designation of a primary or central location</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#LocationFixture"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
