    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#WithinPhysicalEnvironment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Within Physical Environment</skos:prefLabel>
    <skos:definition xml:lang="en">Location is local and entirely within a physical environment, such as a room</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:scopeNote xml:lang="en">This concept will be deprecated in a future DPV release, it is suggested to use other concepts that also represent the nature of the location e.g. dpv:PrivateSpace or its accessibility dpv:PubliclyAccessibleSpace</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-06</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-05-04</dct:modified>
    <sw:term_status xml:lang="en">sunset</sw:term_status>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#PublicLocation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Public Location (deprecated)</skos:prefLabel>
    <skos:definition xml:lang="en">Location that is or can be accessed by the public</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#LocalLocation"/>
    <skos:scopeNote xml:lang="en">This concept will be deprecated in a future DPV release, it is suggested to use the better defined and legally aligned dpv:PublicSpace instead</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-05-04</dct:modified>
