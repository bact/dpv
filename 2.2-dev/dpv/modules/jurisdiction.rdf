  <rdf:Description rdf:about="https://w3id.org/dpv#WithinVirtualEnvironment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Within Virtual Environment</skos:prefLabel>
    <skos:definition xml:lang="en">Location is local and entirely within a virtual environment, such as a software system</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:scopeNote xml:lang="en">This concept will be deprecated in a future DPV release, it is suggested to use the existing concepts representing device as tech:Software and its taxonomy to have parity and consistency between the use of software as a technology and as a virtual environment for data storage and operations</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-06</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-05-04</dct:modified>
    <sw:term_status xml:lang="en">sunset</sw:term_status>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#Location">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Location</skos:prefLabel>
    <skos:definition xml:lang="en">A location is a position, site, or area where something is located</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:scopeNote xml:lang="en">Location may be geographic, physical, or virtual.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples#E0060"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#FixedMultipleLocations">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LocationFixture"/>
    <skos:prefLabel xml:lang="en">Fixed Multiple Locations</skos:prefLabel>
    <skos:definition xml:lang="en">Location that is fixed with multiple places e.g. multiple cities</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#FixedLocation"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
