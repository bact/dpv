    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#PersonalSpace">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Personal Space</skos:prefLabel>
    <skos:definition xml:lang="en">A private space associated with an individual in a personal capacity - such as their home or the space around their physical person e.g. my home or my room</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#PrivateSpace"/>
