  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RightExerciseNotice">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Right Exercise Notice</skos:prefLabel>
    <skos:definition xml:lang="en">Information associated with exercising of an active right such as where and how to exercise the right, information required for it, or updates on an exercised rights request</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">This concept is intended for providing information regarding a right exercise. For specific instances of such exercises, see RightExerciseActivity and RightExerciseRecord.</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightNotice"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RightExerciseActivity">
