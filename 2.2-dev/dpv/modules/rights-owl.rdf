  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RightNonFulfilmentNotice">
    <dct:contributor rdf:nodeID="Nf5762b8c65924f06b8c9fb3009ac4a52"/>
    <dct:contributor rdf:nodeID="N19ec0bf4ff6f475a99f71a33ca13e0ee"/>
