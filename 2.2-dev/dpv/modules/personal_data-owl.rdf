    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Data deemed sensitive</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Data"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InferredPersonalData">
    <skos:definition xml:lang="en">Personal Data that is obtained through inference from other data</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Inferred Data is derived data generated from existing data, but which did not originally exist within it, e.g. inferring demographics from browsing history.</skos:scopeNote>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0009"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <skos:prefLabel xml:lang="en">Inferred Personal Data</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DerivedPersonalData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#InferredData"/>
  </rdf:Description>
