    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-08-16</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <skos:prefLabel xml:lang="en">Rights Exercise Prevented</skos:prefLabel>
    <skos:definition xml:lang="en">Actions or measures that prevent an individual or group from exercising their legal rights.</skos:definition>
